{
    "compilerOptions": {
        "target": "ES2022",
        "lib": [
            "ES2022"
        ],
        "module": "ESNext",
        "moduleResolution": "bundler",
        "strict": true,
        "skipLibCheck": true,
        "noEmit": true,
        "allowImportingTsExtensions": true,
        "isolatedModules": true,
        "allowJs": true,
        "checkJs": false,
        "types": [
            "@cloudflare/workers-types"
        ]
    },
    "include": [
        "functions"
    ]
}